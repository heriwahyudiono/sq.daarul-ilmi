                    <div class='text-1xl'>
                      <%= biodata.fakultas %>
                    </div>
                    </p>
                    <p>
                    <div>Program Studi: </div>
                    <div class='text-1xl'>
                      <%= biodata.program_studi %>
                    </div>
                    </p>
                    <p>
                    <div>Angkatan: </div>
